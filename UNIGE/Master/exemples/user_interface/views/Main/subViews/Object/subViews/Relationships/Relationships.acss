.Main_Object_Relationships {

	table {
		width: 100%;
	}
}

.Main_Object_Relationships_Relationship {
	width: 100%;
	height: 26px;
	line-height: 26px;
	border-bottom: 1px solid rgba(240, 240, 240, 1);
	
	:nth-child(2n) {
		background-color: rgba(255, 255, 255, 1);
	}
	
	:nth-child(2n + 1) {
		background-color: rgba(245, 245, 245, 1);
	}
	
	.objectId {
		width: 30%;
		
		span {
			margin-left: 8px;
			color: #2d3b5a;
		}
		
	}
}